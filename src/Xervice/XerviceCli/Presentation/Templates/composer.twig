{
  "name"             : "{{ namespace|lower }}/{{ origname|lower }}",
  "license"          : "MIT",
  "authors"          : [
    {
      "name" : "Author name",
      "email": "author@email.de"
    }
  ],
  "require-dev": {
    "codeception/codeception": "*",
    "symfony/var-dumper": "*",
    "phpstan/phpstan": "*"
  },
  "require": {
    "php": ">=7.1.0",
    "xervice/core": "^4.0.0"
    "xervice/data-provider": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "DataProvider\\": "src/Generated/"
    },
    "psr-0": {
      "{{ namespace }}\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "{{ namespace }}Test\\": "tests/{{ namespace }}/"
    }
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse src --level=7"
  }
}