{
  "name"             : "{{ namespace|lower }}/{{ name|lower }}",
  "license"          : "MIT",
  "authors"          : [
    {
      "name" : "Author name",
      "email": "author@email.de"
    }
  ],
  "require-dev": {
    "codeception/codeception": "*",
    "symfony/var-dumper": "*"
  },
  "require": {
    "php": ">=7.1.0",
    "xervice/core": "dev-master",
    "xervice/console": "dev-master",
    "xervice/data-provider": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "{{ namespace }}Test\\": "tests/{{ namespace }}/",
      "DataProvider\\": "src/Generated/"
    },
    "psr-0": {
      "{{ namespace }}\\": "src/"
    }
  }
}